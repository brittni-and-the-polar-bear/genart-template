  "description": "A template for new projects using the @batpb/genart library with TypeScript.",
  "private": true,
  "scripts": {
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "stats:dev": "webpack --mode development --profile --json > ./out/stats.json",
    "stats:prod": "webpack --mode production --profile --json > ./out/stats.json",
    "dev": "webpack-dev-server --mode development",
    "lint:js": "eslint -c eslint.config.js.mjs ./eslint.config.js.mjs ./eslint.config.ts.mjs ./webpack.config.cjs",
    "lint:ts": "eslint -c eslint.config.ts.mjs ./src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brittni-and-the-polar-bear/genart-template.git"
  },
  "keywords": [
