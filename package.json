    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brittni-and-the-polar-bear/genart-template/issues"
  },
  "homepage": "https://github.com/brittni-and-the-polar-bear/genart-template#readme",
  "dependencies": {
    "@batpb/genart": "^2.0.0-rc.3-alpha.0",
