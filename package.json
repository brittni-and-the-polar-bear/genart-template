    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/p5": "^1.7.6",
    "css-loader": "^7.1.2",
    "eslint": "^9.35.0",
    "eslint-plugin-es-x": "^9.1.0",
    "eslint-plugin-n": "^17.22.0",
    "eslint-plugin-security": "^3.0.1",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.4",
    "ts-loader": "^9.5.4",
