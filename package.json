    "code art",
    "genart",
    "generative art",
    "p5",
    "p5.js",
    "p5-js",
    "p5.js library",
    "typescript",
    "template"
  ],
  "author": {
    "name": "brittni and the polar bear",
    "url": "https://brittni-and-the-polar-bear.github.io/"
  },
  "contributors": [
